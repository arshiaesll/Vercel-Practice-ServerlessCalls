{
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/home",
            "dest": "/api/index.py"
        },
        {
            "src": "/pdf_extractor",
            "dest": "/api/func_pdf_extractor.py"
        }

    ]

}